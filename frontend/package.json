{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "predev": "yarn gen:gql",
    "dev": "next dev",
    "build": "next build",
    "build:analyze": "cross-env ANALYZE=true next build",
    "start": "next start",
    "lint": "eslint --no-error-on-unmatched-pattern --cache --fix --report-unused-disable-directives \"src/**/*.{js,ts,jsx,tsx}\" \"app/**/*.{js,ts,jsx,tsx}\"",
    "ts:check": "tsc --pretty --skipLibCheck --noEmit",
    "gen:theme": "chakra-cli tokens src/theme/index.ts --strict-component-types",
    "gen:theme:watch": "yarn run theme --watch",
    "gen:gql": "graphql-codegen --config codegen.ts",
    "gen:gql:watch": "yarn run gen:gql --watch",
    "postinstall": "yarn run gen:theme",
    "prebuild": "yarn run gen:gql",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "dependencies": {
    "@apollo/client": "^3.8.8",
    "@chakra-ui/anatomy": "^2.2.2",
    "@chakra-ui/icons": "^2.1.1",
    "@chakra-ui/next-js": "^2.2.0",
    "@chakra-ui/react": "^2.8.2",
    "@chakra-ui/system": "^2.6.2",
    "@chakra-ui/theme-tools": "^2.1.2",
    "@chakra-ui/utils": "^2.0.14",
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@types/node": "20.8.3",
    "@types/react": "18.2.25",
    "@types/react-dom": "18.2.11",
    "apollo-link-error": "^1.1.13",
    "apollo-link-http-common": "^0.2.16",
    "apollo-upload-client": "^17.0.0",
    "axios": "^1.6.2",
    "framer-motion": "^10.16.16",
    "graphql": "^16.8.1",
    "js-cookie": "^3.0.5",
    "next": "13.5.4",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-icons": "^4.12.0",
    "type-fest": "^4.8.3",
    "typescript": "5.2.2"
  },
  "devDependencies": {
    "@chakra-ui/cli": "^2.4.1",
    "@graphql-codegen/add": "^5.0.0",
    "@graphql-codegen/cli": "^5.0.0",
    "@graphql-codegen/client-preset": "^4.1.0",
    "@graphql-codegen/fragment-matcher": "^5.0.0",
    "@graphql-codegen/introspection": "4.0.0",
    "@graphql-codegen/typescript-apollo-client-helpers": "^3.0.0",
    "@graphql-eslint/eslint-plugin": "^3.20.1",
    "@liinkiing/eslint-config": "^1.2.11",
    "@liinkiing/prettier-config": "^1.3.0",
    "@next/bundle-analyzer": "^14.0.4",
    "@parcel/watcher": "^2.3.0",
    "@storybook/addon-essentials": "^7.6.6",
    "@storybook/addon-interactions": "^7.6.6",
    "@storybook/addon-links": "^7.6.6",
    "@storybook/addon-styling": "^1.3.7",
    "@storybook/blocks": "^7.6.6",
    "@storybook/manager-api": "^7.6.6",
    "@storybook/nextjs": "^7.6.6",
    "@storybook/react": "^7.6.6",
    "@storybook/testing-library": "^0.2.2",
    "@storybook/theming": "^7.6.6",
    "@types/apollo-upload-client": "^17.0.5",
    "@types/js-cookie": "^3.0.6",
    "dotenv-load": "^3.0.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "eslint-plugin-storybook": "^0.6.15",
    "prettier": "^2.8.8",
    "storybook": "^7.6.6"
  },
  "prettier": "@liinkiing/prettier-config"
}
